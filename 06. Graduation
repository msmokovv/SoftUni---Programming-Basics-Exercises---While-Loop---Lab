function solve(input) {
  let name = input.shift();
  let counter = 1;
  let sum = 0;
  
  while(counter <= 12){
    let grade = Number(input.shift());
    if(grade >= 4.00){
      sum = sum + grade;
      counter++;
    }
  }
    let average = sum / 12;
  console.log(`${name} graduated. Average grade: ${average.toFixed(2)}`);
  
}
